<view class="component tab-group container">
  <van-tabbar class="tabbar" active="{{active}}" bind:change="onChange">
    <van-tabbar-item wx:for="{{tabs}}" data-route="{{item.route}}" wx:key="{{index}}" class="tab-item {{index === active ? 'active' : 'normal'}}" icon="{{item.icon}}">
      {{item.text}}
    </van-tabbar-item>
  </van-tabbar>
</view>